<?xml version="1.0" standalone="yes"?>
<NewDataSet>
  <xs:schema id="NewDataSet" xmlns="" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
    <xs:element name="NewDataSet" msdata:IsDataSet="true" msdata:MainDataTable="Projects" msdata:UseCurrentLocale="true">
      <xs:complexType>
        <xs:choice minOccurs="0" maxOccurs="unbounded">
          <xs:element name="Projects">
            <xs:complexType>
              <xs:sequence>
                <xs:element name="project_id" msdata:ReadOnly="true" msdata:AutoIncrement="true" msdata:AutoIncrementSeed="1" type="xs:int" />
                <xs:element name="project_name">
                  <xs:simpleType>
                    <xs:restriction base="xs:string">
                      <xs:maxLength value="100" />
                    </xs:restriction>
                  </xs:simpleType>
                </xs:element>
                <xs:element name="date_started" type="xs:dateTime" minOccurs="0" />
                <xs:element name="date_ended" type="xs:dateTime" minOccurs="0" />
                <xs:element name="design_width" type="xs:int" />
                <xs:element name="design_height" type="xs:int" />
                <xs:element name="fabric_width" type="xs:int" />
                <xs:element name="fabric_height" type="xs:int" />
                <xs:element name="fabric_colour" type="xs:int" />
                <xs:element name="design_file">
                  <xs:simpleType>
                    <xs:restriction base="xs:string">
                      <xs:maxLength value="100" />
                    </xs:restriction>
                  </xs:simpleType>
                </xs:element>
                <xs:element name="origin_x" type="xs:int" />
                <xs:element name="origin_y" type="xs:int" />
                <xs:element name="total_minutes" type="xs:int" />
                <xs:element name="fabric_count" type="xs:int" />
              </xs:sequence>
            </xs:complexType>
          </xs:element>
        </xs:choice>
      </xs:complexType>
      <xs:unique name="Constraint1" msdata:PrimaryKey="true">
        <xs:selector xpath=".//Projects" />
        <xs:field xpath="project_id" />
      </xs:unique>
    </xs:element>
  </xs:schema>
 </NewDataSet>